---
pagination:
  data: collections.posts
  size: 1
  alias: post
permalink: "/emails/{{ post.fileSlug }}/"
layout: layouts/post-email.njk

eleventyComputed:
  title: "{{ post.data.title }}"
  date: "{{ post.date }}"
  webUrl: "{{ post.url }}"
  transformed: "{{ post.templateContent | emailifyMarginNotes }}"
  emailContent: "{{ transformed.content }}"
  endnotes: "{{ transformed.endnotes }}"
---

{# Intentionally empty; layout renders emailContent/endnotes #}

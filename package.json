{
	"name": "eleventy-base-blog",
	"version": "9.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"assets:organize:dry": "node _config/organize-local-post-assets.mjs --dry-run",
		"assets:organize": "node _config/organize-local-post-assets.mjs",
		"assets:mirror": "node _config/mirror-post-assets.mjs --clean",
		"build": "npm run assets:mirror && npx @11ty/eleventy",
		"build-nocolor": "npm run assets:mirror && cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npm run assets:mirror && npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"start": "npm run assets:mirror && npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npm run assets:mirror && npx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
		"debug": "npm run assets:mirror && cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "npm run assets:mirror && cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "npm run assets:mirror && cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"assets:localize:dry": "node _config/localize-substack-images.mjs --dry-run",
		"assets:localize": "node _config/localize-substack-images.mjs",
		"assets:localize:all:dry": "node _config/localize-substack-images.mjs --include-remote --dry-run",
		"assets:localize:all": "node _config/localize-substack-images.mjs --include-remote"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"cross-env": "^7.0.3",
		"luxon": "^3.6.1",
		"prismjs": "^1.30.0",
		"zod": "^3.25.67",
		"zod-validation-error": "^3.5.2"
	},
	"dependencies": {
		"@11ty/eleventy-fetch": "^5.1.1",
		"@zachleat/heading-anchors": "^1.0.5",
		"markdown-it": "^14.1.0"
	}
}
